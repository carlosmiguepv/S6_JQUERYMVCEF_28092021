<div id="divListadoClientes">


</div>
<script>
    function CaragarListadoCliente() {
        $.ajax({
            type: "GET",
            url: '@Url.Action("Listado","Customer")',
            cache: false,
            success: function (dataHTML) {
                $('#divListadoClientes').html(dataHTML);
            }
        })
    }

</script>